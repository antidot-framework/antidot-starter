{
  "name": "antidot-fw/antidot-framework-starter",
  "type": "project",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "kpicaza"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/kpicaza/psr11-monolog.git"
    }
  ],
  "require": {
    "php": "^7.2.13",
    "ext-json": "*",
    "ext-yaml": "*",
    "antidot-fw/aura-container-config": "dev-master",
    "antidot-fw/aura-router-adapter": "dev-master",
    "antidot-fw/cli": "dev-master",
    "antidot-fw/dev-tools": "dev-master",
    "antidot-fw/event-dispatcher": "^1.1.0",
    "antidot-fw/framework": "dev-master",
    "antidot-fw/logger": "^1.0.1",
    "antidot-fw/symfony-config-translator": "dev-master",
    "symfony/yaml": "^4.2.7",
    "wshafer/psr11-monolog": "@dev",
    "zendframework/zend-config": "^3.2"
  },
  "require-dev": {
    "franzl/whoops-middleware": "^1.1",
    "phpro/grumphp": "^0.15.0",
    "phpstan/phpstan": "^0.11.5",
    "phpunit/phpunit": "^8.0",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "^3.4",
    "symfony/var-dumper": "^4.2",
    "zendframework/zend-component-installer": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTest\\": "test"
    }
  },
  "scripts": {
    "check-all": [
      "@cs-check",
      "@test",
      "@inspect"
    ],
    "cs-check": "phpcs src --colors",
    "cs-fix": "phpcbf src --colors",
    "inspect": "phpstan analyse src -l7 --ansi",
    "test": "phpunit --colors=always"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "zf": {
      "component-whitelist": [
        "antidot-fw/framework",
        "antidot-fw/logger",
        "antidot-fw/dev-tools",
        "antidot-fw/doctrine",
        "antidot-fw/session",
        "antidot-fw/aura-router-adapter",
        "antidot-fw/cli",
        "antidot-fw/phug-te,plate-renderer",
        "antidot-fw/event-dispatcher",
        "antidot-fw/symfony-config-translator",
        "wshafer/psr11-monolog",
        "zendframework/zend-httphandlerrunner"
      ]
    }
  }
}
